    "def solution(n, k):\n",
    "    d = [0] * 100\n",
    "    \n",
    "    d[0] = k[0]\n",
    "    d[1] = max(k[0], k[1])\n",
    "    for i in range(2, n):\n",
    "        d[i] = max(d[i-1], d[i-2]+k[i])\n",
    "        \n",
    "    return d[n-1]"
   ]
